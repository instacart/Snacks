{
  "name": "snacks",
  "version": "0.1.0",
  "description": "The Instacart Component Library for Web",
  "main": "src/index.js",
  "scripts": {
    "test": "jest --coverage",
    "start": "webpack --progress --watch --config webpack.config.js src/index.js dist/snacks.js",
    "build": "npm test && webpack --progress --config webpack.config.js src/index.js dist/snacks.js",
    "test:watch": "npm test -- --watch --bail",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/instacart/Snacks.git"
  },
  "keywords": [
    "design",
    "snacks",
    "instacart",
    "style"
  ],
  "author": "Dominic Cocchiarella",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/instacart/Snacks/issues"
  },
  "homepage": "https://github.com/instacart/Snacks#readme",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.22.0",
    "enzyme": "^2.9.1",
    "jest": "^20.0.4",
    "radium": "^0.18.2",
    "react": "^15.5.0",
    "react-dom": "^15.5.0",
    "react-styleguidist": "^6.0.2",
    "react-test-renderer": "^15.5.0",
    "sinon": "^2.3.8",
    "underscore": "^1.8.3",
    "webpack": "^2.2.0"
  },
  "dependencies": {
    "underscore": "^1.8.3"
  },
  "peerDependencies": {
    "prop-types": "^15.5.10",
    "radium": "^0.18.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/lib/styleguide/themeSetup.js"
    ]
